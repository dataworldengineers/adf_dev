{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "adf-team-mavericks"
		},
		"ls_SqlServer_onprem_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'ls_SqlServer_onprem'",
			"defaultValue": "Integrated Security=True;Data Source=SIVAKRISHNA\\SQLEXPRESS;Initial Catalog=AdventureWorksDW2014"
		},
		"ls_SqlServer_onprem_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'ls_SqlServer_onprem'"
		},
		"ls_ablob_covidreportingsa_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'ls_ablob_covidreportingsa'"
		},
		"ls_adls_covidreportingdl_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'ls_adls_covidreportingdl'"
		},
		"ls_SqlServer_onprem_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "SIVAKRISHNA\\ravuri.sivakrishna"
		},
		"ls_adls_covidreportingdl_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://adlsteammavericks.dfs.core.windows.net/"
		},
		"tr_ingesr_population_data_properties_typeProperties_scope": {
			"type": "string",
			"defaultValue": "/subscriptions/554c41fe-aca1-46fd-b4a0-159603547de2/resourceGroups/rs_dev_team_mavericks/providers/Microsoft.Storage/storageAccounts/bsteammavericks"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "adf-team-mavericks_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"ls_ablob_covidreportingsa_connectionString": {
						"value": "[parameters('ls_ablob_covidreportingsa_connectionString')]"
					},
					"ls_adls_covidreportingdl_accountKey": {
						"value": "[parameters('ls_adls_covidreportingdl_accountKey')]"
					},
					"ls_SqlServer_onprem_connectionString": {
						"value": "[parameters('ls_SqlServer_onprem_connectionString')]"
					},
					"ls_SqlServer_onprem_password": {
						"value": "[parameters('ls_SqlServer_onprem_password')]"
					},
					"ls_adls_covidreportingdl_properties_typeProperties_url": {
						"value": "[parameters('ls_adls_covidreportingdl_properties_typeProperties_url')]"
					},
					"tr_ingesr_population_data_properties_typeProperties_scope": {
						"value": "[parameters('tr_ingesr_population_data_properties_typeProperties_scope')]"
					},
					"ls_SqlServer_onprem_properties_typeProperties_userName": {
						"value": "[parameters('ls_SqlServer_onprem_properties_typeProperties_userName')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}